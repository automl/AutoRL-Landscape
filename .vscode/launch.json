{
  "version": "0.2.0",
  "inputs": [
    {
      "id": "dataFile",
      "type": "pickString",
      "description": "Data file for run: ",
      "options": [
        "data/kwie98_paper/dqn_ls3_tnt_3.csv",
        "data/kwie98_lls/sac_hopper.csv",
        "data/kwie98_paper/test_resume_1.csv"
      ]
    },
    {
      "id": "combo",
      "type": "pickString",
      "description": "Combo: ",
      "options": [
        "dqn_cartpole",
        "sac_hopper"
      ]
    },
    {
      "id": "ls",
      "type": "pickString",
      "description": "LS: ",
      "options": [
        "dqn",
        "sac"
      ]
    }
  ],
  "configurations": [
    {
      "name": "phases dl",
      "type": "python",
      "request": "launch",
      "program": "${env:CONDA_PREFIX}/bin/phases",
      "console": "integratedTerminal",
      "args": [
        "dl",
        "kwie98",
        "paper",
        "foo"
      ],
      "justMyCode": false
    },
    {
      "name": "phases ana maps",
      "type": "python",
      "request": "launch",
      "program": "${env:CONDA_PREFIX}/bin/phases",
      "console": "integratedTerminal",
      "args": [
        "ana",
        "maps",
        "${workspaceFolder}/${input:dataFile}",
        "ilm"
      ],
      "justMyCode": false
    },
    {
      "name": "phases ana maps cherrypick",
      "type": "python",
      "request": "launch",
      "program": "${env:CONDA_PREFIX}/bin/phases",
      "console": "integratedTerminal",
      "args": [
        "ana",
        "maps",
        "${workspaceFolder}/${input:dataFile}",
        "ilm",
        "--cherry-pick",
        "[(2, 2)]"
      ],
      "justMyCode": false
    },
    {
      "name": "phases ana modalities",
      "type": "python",
      "request": "launch",
      "program": "${env:CONDA_PREFIX}/bin/phases",
      "console": "integratedTerminal",
      "args": [
        "ana",
        "modalities",
        "${workspaceFolder}/${input:dataFile}"
      ],
      "justMyCode": false
    },
    {
      "name": "phases ana concavity",
      "type": "python",
      "request": "launch",
      "program": "${env:CONDA_PREFIX}/bin/phases",
      "console": "integratedTerminal",
      "args": [
        "ana",
        "concavity",
        "${workspaceFolder}/${input:dataFile}",
        "ilm"
      ],
      "justMyCode": false
    },
    {
      "name": "phases ana graphs",
      "type": "python",
      "request": "launch",
      "program": "${env:CONDA_PREFIX}/bin/phases",
      "console": "integratedTerminal",
      "args": [
        "ana",
        "graphs",
        "${workspaceFolder}/${input:dataFile}",
        "ilm"
      ],
      "justMyCode": false
    },
    {
      "name": "phases run",
      "type": "python",
      "request": "launch",
      "program": "${env:CONDA_PREFIX}/bin/phases",
      "console": "integratedTerminal",
      "args": [
        "run",
        "combo=${input:combo}",
        "ls=${input:ls}",
        "slurm=debug",
        "phases=quick",
        "num_confs=1",
        "num_seeds=1",
        "wandb.experiment_tag=debug"
      ],
      "justMyCode": false
    },
    {
      "name": "phases resume",
      "type": "python",
      "request": "launch",
      "program": "${env:CONDA_PREFIX}/bin/phases",
      "console": "integratedTerminal",
      "args": [
        "resume",
        "${workspaceFolder}/${input:dataFile}"
      ],
      "justMyCode": false
    }
  ]
}
